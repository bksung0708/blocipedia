<%= form_for @wiki do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :body %>
  <%= f.text_area :body %>

  <div class="form-group">
    <%= render partial: 'form', locals: { wiki: @wiki } %>
  </div>

<% end %>

<%= render partial: 'collaborators/form', locals: { wiki: @wiki} %>
